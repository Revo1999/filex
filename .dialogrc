# Tag key color
tag_key_color = (WHITE,WHITE,OFF)

# Selected tag key color
tag_key_selected_color = (GREEN,GREEN,ON)

# Check box color
check_color = (WHITE,GREEN,OFF)

# Selected check box color
check_selected_color = (GREEN,WHITE,ON)

# Up arrow color
uarrow_color = (WHITE,GREEN,ON)

# Down arrow color
darrow_color = (WHITE,GREEN,ON)

# Item help-text color
itemhelp_color = (WHITE,GREEN,OFF)

# Active form text color
